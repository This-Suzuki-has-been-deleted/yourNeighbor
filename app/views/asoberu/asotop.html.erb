<script>
var map;
function initMap() {
	map = new google.maps.Map(document.getElementById('map'), { // #mapに地図を埋め込む
		center: { // 地図の中心を指定
			lat: 43.0560242, // 緯度
			lng: 141.3795115, // 経度
		},
		zoom: 18 // 地図のズームを指定
	});
}
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBJfuH3GcpFYapsQGKpWvoiZJ2oqF8ud44&callback=initMap"></script>

