<%= render "shared/errors", obj: @maps %>

<table class="attr">
  <tr>
    <th><%= form.label :map_name, "地点名" %></th>
    <td><%= form.text_field :map_name, size: 15 %></td>
  </tr>
  <tr>
    <th>緯度経度</th>
    <td>
      <%= form.label :map_lat, "緯度" %>
      <%= form.text_field :map_lat, size: 20 %>
      <%= form.label :map_lng, "経度" %>
      <%= form.text_field :map_lng, size: 20 %>
    </td>
  </tr>
</table>
